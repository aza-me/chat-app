generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int       @id @default(autoincrement())
  password     String
  username     String?   @unique
  email        String    @unique
  firstName    String?
  lastName     String?
  phone        String?   @unique
  bio          String?
  birthdayDate DateTime?
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
}
